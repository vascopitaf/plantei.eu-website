<h2>{{lang 'seedbank::messages.news'}}</h2>
  <table class="table" id="news">
    <tbody>
      {{#each news}}
      <tr data-message_id="{{id}}">
        <td>{{thread.subject}}</td>
        <td>{{user.name}}</td>
        <td>{{updated_at}}</td>
      </tr>
      {{else}}
      <tr>
        <h4>{{lang 'seedbank::messages.noContent'}}</h4>
      </tr>
      {{/each}}
    </tbody>
  </table>
